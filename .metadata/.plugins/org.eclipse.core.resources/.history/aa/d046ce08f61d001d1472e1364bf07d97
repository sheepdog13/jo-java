package chap09_interface;

public class InterfaceTest {
	public static void main(String[] args) {
		// 인터페이스를 구현하여 작성한 클래스는 서브클래스와 동일하게 쓰인다
		// 상속과 인터페이스의 차이점
		// 상속은 부모클래스의 필드/메소드를 가져다 쓰고 필요한 부분을 오버라이딩
		// 인터페이스는 인터페이스의 대부분의 추상메소드를 오버라이딩
		Truck t1 = new Truck();
		t1.move();
		t1.printCar();
	}
}
