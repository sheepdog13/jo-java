package practice;

public class Game {
	private String gameTitle;
	
	Game(){
		this.gameTitle = "미정";
	}
	Game(String gameTitle){
		this.gameTitle = gameTitle;
	}
	
	String getGameTitle() {
		return this.gameTitle;
	}
	void play() {
		System.out.println("게임을 시작합니다");
	}
	void stop() {
		System.out.println("게임을 멈춥니다");
	}
	void end() {
		System.out.println("게임을 종료합니다");
		}
	}

